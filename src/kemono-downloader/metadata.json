{
    "name": "Kemono Downloader",
    "description": "Download images and attachments from posts on Kemono and Coomer",
    "version": "2.1.0",
    "namespace": "kemono",
    "match": ["*://kemono.cr/*", "*://coomer.st/*"],
    "iconURL": " https://www.google.com/s2/favicons?sz=64&domain=kemono.party",
    "run-at": "document-start",
    "grant": ["window.onurlchange", "GM_download", "GM_notification"]
}
